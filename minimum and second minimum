//Write the code here.
#include<stdio.h>
void main(){
	int a[10],n,i,min,secmin;
	printf("No.of values: ");
	scanf("%d",&n);
	printf("Elements: ");
	for(i = 0;i < n;i++){
		scanf("%d",&a[i]);
	}

	if(a[0] < a[i]){
		min = a[0];
		secmin = a[1];
	}
	else{
		min = a[1];
		secmin = a[0];
	}
	for(i = 2;i < n;i++){
		if(a[i] < min){
			secmin = min;
			min = a[i];
		}
		else if(a[i] > min && a[i] < secmin){
			secmin = a[i];
		}
	}
	printf("Min element= %d\n",min);
	printf("Second min element= %d\n",secmin);
}
